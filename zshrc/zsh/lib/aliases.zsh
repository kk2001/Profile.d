# Basic directory operations
alias ...='cd ../..'

#alias g='grep -in'

# Show history
alias history='fc -l 1'

# List direcory contents
alias lsa='ls -lah'
alias l='ls -la'
alias ll='ls -l'

alias p='cd ~/Projects'
alias d='cd ~/Downloads'

alias psf='ps f'
alias gp='git pull'

[ -f /usr/local/bin/mvim ] && alias vim='mvim -v'
[ -f /usr/local/bin/mvim ] && alias gvim='mvim'
